	<script>
	var app = angular.module("twitter_profile_app", []); 
	app.controller("twitter_profile_controller", function($scope,$http,$window) {
		
		$scope.follower = false;
		$scope.tweets = true;
		$scope.following = false;
		$scope.editProfile = false;
		
		
		$http({
			method:'get',
			url:'/profileData'
		}).then(function(res){
			if(res.data.status == "200"){
				$scope.userDetails = {"firstName":res.data.userDetails.firstName,"lastName":res.data.userDetails.lastName,"twitterHandle":res.data.userDetails.twitterHandle};
				$scope.followingCount=res.data.followingCount;
				$scope.followerCount=res.data.followerCount;
				$scope.tweetsCount=res.data.tweetsCount;
				$scope.tweetArray = [];
				$scope.checkingHashTagInTweets(res,function(){
					;
				});
			}else{
				$window.location.href = '/signIn';
			}
			
		})
		
		$scope.followingShow=function(){
			
			$http({
				method:'get',
				url:'/followingData'
			}).then(function(res){
				if(res.data.status == "200"){
					$scope.followingArray = [];
					for(var i=0;i< res.data.followingArray.length;i++){
						$scope.followingArray[i] = {"followingName":res.data.followingArray[i].name,"followingTwitterHandle":res.data.followingArray[i].followingTwitterHandle,"followingEmail":res.data.followingArray[i].following};
					}
					$scope.tweets = false;
					$scope.follower = false;
					$scope.editProfile = false;
					$scope.following = true;
				}else{
					$window.location.href = '/signIn';
				}
				
			})	
		}
		
		$scope.followerShow=function(){
			
			$http({
				method:'get',
				url:'/followerData'
			}).then(function(res){
				if(res.data.status == "200"){
					$scope.followerArray = [];
					var j=0,i=0;
					while(i<res.data.followerArrayShowButton.length){
						$scope.followerArray[i] = {"followerName":res.data.followerArrayShowButton[i].name,"followerTwitterHandle":res.data.followerArrayShowButton[i].followerTwitterHandle,"followerEmail":res.data.followerArrayShowButton[i].follower,"followButton":true};
						i++;
					}
					while(j<res.data.followerArrayHideButton.length){
						$scope.followerArray[i] = {"followerName":res.data.followerArrayHideButton[j].name,"followerTwitterHandle":res.data.followerArrayHideButton[j].followerTwitterHandle,"followerEmail":res.data.followerArrayHideButton[j].follower,"followButton":false};
						j++;
						i++;
					}
					$scope.tweets = false;
					$scope.follower = true;
					$scope.following = false;
					$scope.editProfile = false;
				}else{
					$window.location.href = '/signIn';
				}
				
			})	
			
		}
		
		$scope.home = function(){
			$window.location.href = '/home';
		}
		
		$scope.profile = function(){
			$http({
				method:'get',
				url:'/profileData'
			}).then(function(res){
				if(res.data.status == "200"){
					$scope.userDetails = {"firstName":res.data.userDetails.firstName,"lastName":res.data.userDetails.lastName,"twitterHandle":res.data.userDetails.twitterHandle};
					$scope.followingCount=res.data.followingCount;
					$scope.followerCount=res.data.followerCount;
					$scope.tweetsCount=res.data.tweetsCount;
					$scope.tweetArray = [];
					$scope.checkingHashTagInTweets(res,function(){
						;
					});
					$scope.follower = false;
					$scope.tweets = true;
					$scope.following = false;
					$scope.editProfile = false;
				}else{
					$window.location.href = '/signIn';
				}
				
			})
		}
		
		$scope.searchHashTag=function(hashTag){
			
			$scope.searchHashTagValue = hashTag;
			$scope.searchTweet(hashTag,function(){
				;
			})
			
		}
		
		
		$scope.searchTweet = function(hashTag){
			
			$http({
				method:'post',
				url:'/searchTweet',
				data:{"hashTag":hashTag}
			}).then(function(res){
				if(res.data.status == "200"){
					
					if(res.data.tweetArray.length > 0){
						$scope.follower = false;
						$scope.tweets = true;
						$scope.following = false;
						$scope.editProfile = false;
						$scope.checkingHashTagInTweets(res,function(){
							;
						});
					}else{
						$scope.tweetArray = [];
					}
					
				}else{
					$window.location.href = '/signIn';
				}
				
			})
			
		}
		
		$scope.editProfileShow = function(){
			
			$http({
				method:'get',
				url:'/getDetails'
				}).then(function(res){
				if(res.data.status == "200"){
					$scope.editDetails = {"firstName":res.data.userDetails.firstName,"lastName":res.data.userDetails.lastName,"contact":res.data.userDetails.contact,"email":res.data.userDetails.email,"country":res.data.userDetails.country,"state":res.data.userDetails.state,"city":res.data.userDetails.city,"birthDate":res.data.userDetails.birthDate,"twitterHandle":res.data.userDetails.twitterHandle};
					$scope.tweets = false;
					$scope.follower = false;
					$scope.following = false;
					$scope.editProfile = true;
				}else{
					$window.location.href = '/signIn';
				}
			});
			
		}
		
		
		$scope.updateInfo = function(){
			
			$http({
				method:'post',
				url:'/updateDetails',
				data:{"state":$scope.editDetails.state,"country":$scope.editDetails.country,"city":$scope.editDetails.city,"contact":$scope.editDetails.contact,"birthDate":$scope.editDetails.birthDate,"twitterHandle":$scope.editDetails.twitterHandle}
				}).then(function(res){
				if(res.data.status == "200"){
					alert("User Details Updated Successfully !!");
					$scope.editDetails = {"firstName":res.data.userDetails.firstName,"lastName":res.data.userDetails.lastName,"contact":res.data.userDetails.contact,"email":res.data.userDetails.email,"country":res.data.userDetails.country,"state":res.data.userDetails.state,"city":res.data.userDetails.city,"birthDate":res.data.userDetails.birthDate,"twitterHandle":res.data.userDetails.twitterHandle};
					$scope.tweets = false;
					$scope.follower = false;
					$scope.following = false;
					$scope.editProfile = true;
				}else{
					$window.location.href = '/signIn';
				}
			});
			
		}
		
		
		$scope.logout = function(){
			$http({
				method:'get',
				url:'/sessionEnd'
				}).then(function(res){
				if(res.data.status == "200"){
					;
				}else{
					$window.location.href = '/logout';
				}
			});
		}
		
		
		$scope.unfollow=function(unfollowEmail){
			
			$http({
				method:'POST',
				url:'/unfollow',
				data:{"unfollowEmail":unfollowEmail}
			}).then(function(res){
				if(res.data.status =="200"){
					$scope.followingArray = [];
					$scope.followingCount=res.data.followingCount;
					for(var i=0;i< res.data.followingArray.length;i++){
						$scope.followingArray[i] = {"followingName":res.data.followingArray[i].name,"followingTwitterHandle":res.data.followingArray[i].followingTwitterHandle,"followingEmail":res.data.followingArray[i].following};
					}
				}else{
					$window.location.href = '/signIn';
				}
			})
		}
		
		$scope.follow=function(followEmail){
			$http({
				method:'POST',
				url:'/follow',
				data:{"followEmail":followEmail}
			}).then(function(res){
				if(res.data.status =="200"){
					$scope.followingCount=res.data.followingCount;
					$scope.followerArray = [];
					var j=0,i=0;
					while(i<res.data.followerArrayShowButton.length){
						$scope.followerArray[i] = {"followerName":res.data.followerArrayShowButton[i].name,"followerTwitterHandle":res.data.followerArrayShowButton[i].followerTwitterHandle,"followerEmail":res.data.followerArrayShowButton[i].follower,"followButton":true};
						i++;
					}
					while(j<res.data.followerArrayHideButton.length){
						$scope.followerArray[i] = {"followerName":res.data.followerArrayHideButton[j].name,"followerTwitterHandle":res.data.followerArrayHideButton[j].followerTwitterHandle,"followerEmail":res.data.followerArrayHideButton[j].follower,"followButton":false};
						j++;
						i++;
					}
				}else{
					$window.location.href = '/signIn';
				}
			})
			
		}
		
		$scope.checkingHashTagInTweets=function(res){
			
			$scope.tweetArray = [];
			for(var i=0;i< res.data.tweetArray.length;i++){
				
				var arr = (res.data.tweetArray[i].tweet).split('');
				var plainText = "";
				var k=0;
				$scope.tweetArray1 = [];
			
				for(var j=0;j<arr.length;j++){
					
					if(arr[j]=="#"){
						$scope.tweetArray1[k] = {plainText:plainText,hashTag:""};
						plainText = "";
						k++;
						var hashTag = "";
						while(arr[j] != " " && j<arr.length){
							hashTag+=arr[j];
							j++;
							if(arr[j] == "#"){
								j--;
								break;
							}
						}
						$scope.tweetArray1[k] = {plainText:"",hashTag:hashTag};		
						k++;
					}else{
						plainText+=arr[j];
					}
					
				}
				$scope.tweetArray1[k] = {plainText:plainText,hashTag:""};
				
				$scope.tweetArray[i]={"twitterHandle":res.data.tweetArray[i].twitterHandle,"tweetData":$scope.tweetArray1,"tweetId":res.data.tweetArray[i].tweetId};
				
			}
		}
		
		
	});
		
	</script>


